	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"browser": "build/urpflanze.js",
	"files": [
		"build",
		"dist",
		"package.json",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"lint": "eslint src --ext .ts",
		"build": "ttsc",
		"build-library": "ttsc & npx webpack --config ./webpack.config.js --mode development & npx webpack --config ./webpack.config.js --mode production",
		"prepublishOnly": "ttsc & npm run build-library & npm run build-docs",
		"watch": "ttsc --watch",
		"generate-docs-references": "node generate-docs-references.js",
		"serve-docs": "webpack serve --config webpack.config.docs.js",
		"build-docs": "ttsc & npm run generate-docs-references & npx webpack --config webpack.config.docs.js --mode production"
	},
	"publishConfig": {},
	"devDependencies": {
		"@types/uuid": "^8.3.0",
